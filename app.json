{
  "expo": {
    "name": "RatTick",
    "slug": "tick-shit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon-white.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-man.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-man-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rattick.rattick",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
