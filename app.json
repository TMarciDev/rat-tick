{
  "expo": {
    "name": "RAT TICK",
    "slug": "rattick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/man_icon_whites.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/man_icon_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/man_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rattick.rattick",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
